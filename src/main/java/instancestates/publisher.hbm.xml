<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="instancestates.Publisher" table="HB_PUBLISHERS">
		<id name="id" type="int">
			<column name="PUBLISHER_ID" />
			<generator class="native" />
		</id>
		<property name="name" type="java.lang.String" length="30">
			<column name="PUBLISHER_NAME" />
		</property>
		<property name="email" type="java.lang.String" length="30">
			<column name="PUBLISHER_EMAIL" />
		</property>
		<property name="address" type="java.lang.String" length="30">
			<column name="PUBLISHER_ADDRESS" />
		</property>
	</class>
</hibernate-mapping>